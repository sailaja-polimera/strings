#include <stdio.h>
#include<string.h>
int main()
{
    char str1[20],str2[20];
    printf("enter the string:");
    scanf("%s",str1);
    int len,flag=0;
    len=strlen(str1);
    for(int i=len-1;i>=0;i--)
    {
        str2[len-i-1]=str1[i];//strore str1 elements from back in another string 
    }
    for(int i=0;i<len;i++)
    {
        if(str2[i]==str1[i])//if both the strings are equal
        {
            flag=1;
        }
        else
        flag=0;
    }
   if(flag==1)
       printf("palindrome");
   else
   printf("not a palindrome");
    return 0;
}
