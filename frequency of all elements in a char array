#include <stdio.h> 
#include<string.h>
int main()   
{
int i,j;   
char a[56]="sailaja";

int b[] ={0,0,0,0,0,0,0};
int l=strlen(a);
for(i=0;i<l;i++)  
{
    int count=1;
    for(j=i+1;j<l;j++) 
    {
        if(a[i]==a[j])   
        {
        count++; 
        b[j]=-100; 
    }

}
  if(b[i]!=-100)  

  b[i]=count;    
}
    for(i=0;i<l;i++)   

    {
         if(b[i]!=-100) 

        printf("Frequency  of %d index element %c is:%d\n",i,a[i],b[i]);  
    }
    return 0;  
    }
