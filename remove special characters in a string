#include <stdio.h>
char *duplicate(char s[])
{
   int i=0;
 int j=0;
 for(int i=0;s[i]!='\0';i++)
 {
     while(!(  (s[i]>=65 && s[i]<=90) ||  (s[i]>=97 && s[i]<=122)  || (s[i]=='\0')))
     {
         for(j=i;s[j]!='\0';j++)
         {
         s[j]=s[j+1];
         }
         s[j]='\0';
     }
 } 
}
int main()
{
 char s[50];
 printf("Enter a string:");
 scanf("%[^\n]s",s);
 char* (*fptr)(char[])=duplicate;
 (*fptr)(s);
 printf("%s",s);
}
