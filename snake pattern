#include <stdio.h>

int main()
{
    for(int i=0;i<1;i++)
    {
        static int j;
        for(j=0;j<5;j++)
        {
                if(j%2==0)
                printf("%d ",j);
                else
                printf(" ");
                
        }
        break;
        
    }
printf("\n");
for(int i=1;i<2;i++)
    {
        static int j;
        for(j=0;j<5;j++)
        { 
            if(j%2!=0)
                printf("%d ",j);
                else
                printf(" ");
        }break;
    }
            
    return 0;
}
